    <menuitem id="counter" parent="workshop_menu" name="Counter Component" action="counter_view_action"/>
    <menuitem id="lch" parent="workshop_menu" name="Life Cycle Hooks" action="lch_view_action"/>
    <menuitem id="service_menu" parent="workshop_menu" name="Services" action="sample_service_action"/>
    <menuitem id="extend_menu" parent="workshop_menu" name="Counter Extend" action="sample_extend_action"/>
    <menuitem id="use_effect_menu" parent="workshop_menu" name="Use Effect" action="sample_use_effect_action"/>

