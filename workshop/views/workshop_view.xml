<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="parent_view_action" model="ir.actions.client">
        <field name="name">Parent Child Component</field>
        <field name="tag">parent</field>
    </record>

    <record id="counter_view_action" model="ir.actions.client">
        <field name="name">Counter Component</field>
        <field name="tag">counter</field>
    </record>

    <record id="lch_view_action" model="ir.actions.client">
        <field name="name">Life Cycle Hooks</field>
        <field name="tag">life_cycle_hooks</field>
    </record>


    <record id="sample_service_action" model="ir.actions.client">
        <field name="name">Services</field>
        <field name="tag">sample_service</field>
    </record>
     <record id="sample_extend_action" model="ir.actions.client">
        <field name="name">Services</field>
        <field name="tag">counter_extend</field>
    </record>
 <record id="sample_use_effect_action" model="ir.actions.client">
        <field name="name">Use Effect</field>
        <field name="tag">use_effect</field>
    </record>

    <menuitem id="workshop_menu" name="Workshop"/>
