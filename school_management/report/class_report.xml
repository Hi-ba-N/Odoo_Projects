<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <template id="report_class">
       <t t-call="web.html_container">
       <t t-call="web.external_layout">
                       <div class="oe_structure"/>
                       <h2>Class Report</h2>
                       <br/>
                       <table class="table table-condensed">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>School</th>
                    <th>Department</th>

                </tr>
            </thead>
            <tbody>
                <t t-foreach="report" t-as="rep">
                    <tr>
                        <td><t t-esc="rep['name']"/></td>
                        <td><t t-esc="rep['school_id']"/></td>
                        <td><t t-esc="rep['department_name']"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
               </t>

   </template>
</odoo>