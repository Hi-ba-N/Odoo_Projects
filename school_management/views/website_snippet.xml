<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="event_snippet" name="Event Snippet">

<section class="latest_event">

<div class="row">

<div class="text-center py-5">

<div class="h4" style="text-align:center">Latest Events <br/>
    <br/></div>

<div class="col-md-12">

<!--<div class="h3" style="text-align:center">Latest Events</div>-->

<div id="event_latest">

</div>

</div>

</div>

</div>

</section>
</template>


    <template id="basic_snippet" inherit_id="website.snippets"
              name="Latest Event Snippet">
        <xpath expr="//div[@id='snippet_effect']//t[@t-snippet][last()]"
               position="after">
            <t t-snippet="school_management.event_snippet"/>
        </xpath>
    </template>

</odoo>
